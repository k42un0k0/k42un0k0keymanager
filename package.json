{
  "name": "k42un0k0passwordmanager",
  "version": "0.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "ng": "ng",
    "dev": "run-p start:web watch:electron",
    "start": "run-p start:web start:app",
    "start:app": "run-p watch:electron start:electron",
    "start:electron": "cross-env ELECTRON_START_URL=\"http://localhost:4200\" electron --inspect=5858 .",
    "start:web": "ng serve",
    "build:web": "ng build",
    "build:electron": "cpx electron/splash.html dist && tsc -p electron/tsconfig.json",
    "test:web": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch:electron": "yarn build:electron --watch",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "build-storybook": "yarn docs:json && build-storybook",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "private": true,
  "build": {
    "appId": "com.electron.k42un0k0passwordmanager",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "buildResources": "./dist",
      "output": "./build"
    }
  },
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~11.2.8",
