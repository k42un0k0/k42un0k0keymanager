{
  "name": "app",
  "version": "0.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "start": "run-p watch:electron start:electron",
    "start:electron": "cross-env ELECTRON_START_URL=\"http://localhost:4200\" electron --inspect=5858 .",
    "build": "cpx electron/splash.html dist && tsc -p electron/tsconfig.json",
    "watch:electron": "yarn build:electron --watch",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "private": true,
  "build": {
    "appId": "com.electron.k42un0k0passwordmanager",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "buildResources": "./dist",
      "output": "./build"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "electron-reload": "^1.5.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^12.11.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "npm-run-all": "^4.1.5",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.5"
  }
}
