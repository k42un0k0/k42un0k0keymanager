{
  "files.exclude": {
    "front/amplify/.config": true,
    "front/amplify/**/*-parameters.json": true,
    "front/amplify/**/amplify.state": true,
    "front/amplify/**/transform.conf.json": true,
    "front/amplify/#current-cloud-backend": true,
    "front/amplify/backend/amplify-meta.json": true,
    "front/amplify/backend/awscloudformation": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  }
}
